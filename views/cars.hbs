        <p class="cost">{{this.costBYN}}</p>
        <p>{{this.year}}</p>
        <p>{{this.mileage}}</p>
    </div>
    <div class="info_block i2">
        <p>{{this.engine–°apacity}} / {{this.enginePower}} / {{this.fuelType}}</p>
